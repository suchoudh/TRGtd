            </file>
        </folder>
    </folder>
    
    <folder name="Shortcuts">
        <file name="DS-2.shadow">
            <attr name="originalFile" stringvalue="Actions/Process/tr-view-process-ProcessThoughtsAction.instance"/>
        </file>
        <file name="C-W.shadow">
            <attr name="originalFile" stringvalue="Actions/Process/tr-view-process-ProcessAction.instance"/>
        </file>
