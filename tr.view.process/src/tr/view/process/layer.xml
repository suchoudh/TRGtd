        <folder name="Window">
            <file name="org-netbeans-core-windows-actions-CloseWindowAction.instance_hidden"/>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Process">        
            <attr name="SystemFileSystem.localizingBundle" stringvalue="tr.view.process.Bundle"/>                   
