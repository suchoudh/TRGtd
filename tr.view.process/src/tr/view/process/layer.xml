            <file name="process-helpset.xml" url="process-helpset.xml"/>
        </folder>
    </folder>    
    <folder name="Windows2">
        <folder name="Components">
            <file name="ProcessTopComponent.settings" url="ProcessTopComponentSettings.xml"/>
